{
  "name": "cdk",
  "version": "0.1.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "deploy": "npm run deploy:ci && npm run deploy:app",
    "deploy:domain": "cdk deploy 'snort-route53-base*' --require-approval=never",
    "deploy:domain:certificate": "cdk deploy 'snort-route53-cert-*' --require-approval=never",
    "deploy:ci": "cdk deploy 'snort-ci*' --require-approval=never",
    "deploy:app": "cdk deploy 'snort-app*' --require-approval=never",
    "destroy": "npm run build:lambdas && cdk destroy '*' --require-approval=never",
    "build": "tsc && npm run build:lambdas",
    "build:lambdas": "npx webpack -c webpack.config.js",
    "build:lambdas:watch": "npx webpack -c webpack.config.js --watch",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "get:backend-url": "./scripts/get-backend-url.sh"
  },
  "devDependencies": {
    "@aws-cdk/assert": "^1.30.0",
    "@types/jest": "^24.9.1",
    "@types/node": "10.17.5",
    "aws-cdk": "^1.30.0",
    "jest": "^24.9.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.7.0",
    "typescript": "~3.7.2"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "^1.30.0",
    "@aws-cdk/aws-certificatemanager": "^1.30.0",
    "@aws-cdk/aws-cloudfront": "^1.30.0",
    "@aws-cdk/aws-codebuild": "^1.30.0",
    "@aws-cdk/aws-codecommit": "^1.30.0",
    "@aws-cdk/aws-codedeploy": "^1.30.0",
    "@aws-cdk/aws-codepipeline-actions": "^1.30.0",
    "@aws-cdk/aws-dynamodb": "^1.30.0",
    "@aws-cdk/aws-ec2": "^1.30.0",
    "@aws-cdk/aws-ecr": "^1.30.0",
    "@aws-cdk/aws-iam": "^1.30.0",
    "@aws-cdk/aws-lambda": "^1.30.0",
    "@aws-cdk/aws-route53": "^1.30.0",
    "@aws-cdk/aws-route53-targets": "^1.30.0",
    "@aws-cdk/aws-s3": "^1.30.0",
    "@aws-cdk/aws-s3-deployment": "^1.30.0",
    "@aws-cdk/core": "^1.30.0",
    "@mobileposse/auto-delete-bucket": "1.23.0",
    "aws-sdk": "^2.643.0",
    "source-map-support": "^0.5.16",
    "ts-loader": "^6.2.1",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  }
}
