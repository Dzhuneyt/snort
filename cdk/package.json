{
  "name": "cdk",
  "version": "0.1.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "deploy": "npm run deploy:ci && npm run deploy:app",
    "deploy:ci": "npm run build:lambdas && cdk deploy 'snort-ci*' --require-approval=never",
    "deploy:app": "npm run build:lambdas && cdk deploy 'snort-app*' --require-approval=never",
    "build": "tsc && npm run build:lambdas",
    "build:lambdas": "npx webpack -c webpack.config.js",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk"
  },
  "devDependencies": {
    "@aws-cdk/assert": "^1.27.0",
    "@types/jest": "^24.9.1",
    "@types/node": "10.17.5",
    "aws-cdk": "^1.27.0",
    "jest": "^24.9.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.1.0",
    "typescript": "~3.7.2"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "^1.27.0",
    "@aws-cdk/aws-codebuild": "^1.27.0",
    "@aws-cdk/aws-codecommit": "^1.27.0",
    "@aws-cdk/aws-codedeploy": "^1.27.0",
    "@aws-cdk/aws-codepipeline-actions": "^1.27.0",
    "@aws-cdk/aws-dynamodb": "^1.27.0",
    "@aws-cdk/aws-ec2": "^1.27.0",
    "@aws-cdk/aws-ecr": "^1.27.0",
    "@aws-cdk/aws-iam": "^1.27.0",
    "@aws-cdk/aws-lambda": "^1.27.0",
    "@aws-cdk/aws-s3": "^1.27.0",
    "@aws-cdk/core": "^1.27.0",
    "aws-sdk": "^2.639.0",
    "source-map-support": "^0.5.16",
    "ts-loader": "^6.2.1",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  }
}
